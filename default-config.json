{
  "schedule": "0 */15 * * * *",
  "providerDefaults": {
    "dropbox": {
      "token_store": "/data/.dropbox-token"
    }
  },
  "providers": [
    {
      "type": "dropbox"
    }
  ],
  "handlers": [
    {
      "extensions": ["jpg", "jpeg", "png", "gif", "webp"],
      "conversions": {
        "webp": "jpg"
      },
      "target": "/media/photo"
    },
    {
      "extensions": ["mp4", "mov"],
      "target": "/media/video"
    }
  ]
}
